<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
	<servlet>	
		<display-name>EmployeeServlet</display-name>
		<servlet-name>EmployeeServlet</servlet-name>
		<servlet-class>com.tallink.employee.servlets.EmployeeServlet</servlet-class>
	</servlet>	
	
	<servlet>	
		<display-name>RunApiTestServlet</display-name>
		<servlet-name>RunApiTestServlet</servlet-name>
		<servlet-class>com.tallink.employee.servlets.RunApiTestServlet</servlet-class>
	</servlet>
	
	
	<servlet-mapping>
		<servlet-name>EmployeeServlet</servlet-name>
		<url-pattern>/employees</url-pattern>
	</servlet-mapping>	 	
	
	<servlet-mapping>
		<servlet-name>RunApiTestServlet</servlet-name>
		<url-pattern>/tests</url-pattern>
	</servlet-mapping>
	
</web-app>